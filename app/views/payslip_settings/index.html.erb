<div class="row">
  <div class="large-12 small-12 columns">
    <h1>Payslip Setting</h1>

    <table>
      <thead>
        <th>Name</th>
        <th>Value</th>
        <th>Maths</th>
      </thead>
      <tbody>
        <% @payslip_settings.each do |payslip_setting| %>
        <tr>
          <td><%= payslip_setting.name %></td>
          <td><%= payslip_setting.value %></td>
          <td><%= payslip_setting.maths %></td>
        </tr>
        <% end %>
      </tbody>
    </table>

    <%= link_to "Add New Payslip Settings", new_organization_payslip_setting_path(current_organization, @payslip_setting), class: "button"%>
  </div>
</div>